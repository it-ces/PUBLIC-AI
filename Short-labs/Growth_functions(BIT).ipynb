        "# growth\n",
        "import numpy as np\n",
        "def timf(F,N,rate):\n",
        "  \"\"\"\n",
        "  F is final value\n",
        "  N is the initial value\n",
        "  rate is the velocity \"\"\"\n",
        "  num = np.log(F) - np.log(N)\n",
        "  return num/np.log(rate)"
      ],
