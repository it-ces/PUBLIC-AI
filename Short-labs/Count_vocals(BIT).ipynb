        "def count_vowels(iterable):\n",
        "  vowel=0\n",
        "  for i in iterable:\n",
        "    if i in ['a','e','i','o','u']:\n",
        "      vowel += 1\n",
        "  return vowel\n",
        "print(count_vowels('abcdd'))"
      ]
